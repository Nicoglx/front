    "apollo-server-core": "^2.15.1",
    "apollo-server-micro": "^2.15.1",
    "axios": "^0.19.2",
    "babel-plugin-import-graphql": "^2.7.0",
    "bootstrap": "^4.5.0",
    "date-fns": "^2.14.0",
    "date-fns-tz": "^1.0.10",
    "dotenv": "^8.2.0",
    "easyqrcodejs": "^3.7.1",
    "firebase": "^7.16.0",
    "firebase-admin": "^9.0.0",
    "google-libphonenumber": "^3.2.10",
    "google-map-react": "^1.1.7",
    "graphql": "^15.3.0",
