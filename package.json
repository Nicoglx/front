    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@prisma/cli": "^2.2.2",
    "@prisma/client": "^2.2.2",
    "@sentry/browser": "^5.19.2",
    "@sentry/node": "^5.19.2",
    "@zeit/next-sass": "^1.0.1",
